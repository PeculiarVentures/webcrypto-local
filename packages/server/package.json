{
  "name": "@webcrypto-local/server",
  "version": "1.7.0",
  "description": "A package of `webcrypto-local` framework. WebSocket server",
  "keywords": [
    "webcrypto",
    "server",
    "websocket"
  ],
  "author": "PeculiarVentures",
  "contributors": [
    "Miroshin Stepan<microshine@mail.ru>"
  ],
  "files": [
    "build/**/*.{ts,js}",
    "README.md"
  ],
  "homepage": "https://github.com/PeculiarVentures/webcrypto-local/tree/master/packages/server#readme",
  "license": "MIT",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "types": "build/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeculiarVentures/webcrypto-local.git"
  },
  "scripts": {
    "test": "mocha",
    "build": "npm run build:module && npm run build:types",
    "clear": "rimraf build/*",
    "rebuild": "npm run clear && npm run build",
    "build:module": "rollup -c",
    "build:types": "tsc -p tsconfig.compile.json --outDir build/types --declaration --emitDeclarationOnly"
  },
  "bugs": {
    "url": "https://github.com/PeculiarVentures/webcrypto-local/issues"
  },
  "dependencies": {
    "2key-ratchet": "^1.0.18",
    "@peculiar/json-schema": "^1.1.12",
    "@peculiar/webcrypto": "1.0.22",
    "@peculiar/x509": "^1.6.1",
    "@types/pvutils": "^1.0.1",
    "@webcrypto-local/cards": "^1.7.0",
    "@webcrypto-local/core": "^1.7.0",
    "@webcrypto-local/proto": "^1.7.0",
    "asn1js": "^3.0.3",
    "graphene-pk11": "^2.3.0",
    "node-webcrypto-p11": "^2.4.2",
    "pcsclite": "^1.0.0",
    "pkijs": "^2.3.1",
    "pvtsutils": "^1.3.2",
    "pvutils": "^1.1.3",
    "request": "^2.88.0",
    "tslib": "^2.4.0",
    "webcrypto-core": "^1.7.5",
    "ws": "^8.6.0"
  }
}
